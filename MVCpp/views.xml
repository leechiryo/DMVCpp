<?xml version="1.0" encoding="utf-8"?>
<window id="main_window" text="MVC++ テスト" width="800" height="600">
  <layer>
    <layout gridline="true">
      <rows>
        <row height="50"/>
        <row height="50"/>
        <row height="50"/>
        <row height="50"/>
        <row height="50"/>
        <row height="50"/>
        <row height="50"/>
        <row height="50"/>
        <row height="*"/>
      </rows>
      <cols>
        <col width="200"/>
        <col width="*"/>
        <col width="100"/>
        <col width="10%"/>
      </cols>
    </layout>

    <label id="lbl1" row="1" col="0" text="CSV File:" right="20"/>
    <textbox id="tbx1" row="1" col="1" text="Input ..." left="0" right="50"/>
    <button id="btn1" row="2" col="1" text="START" left="0" />
    <button id="btn2" row="2" col="1" text="IMPORT CSV" left="150" />
    <checkbox id="cbx1" row="3" col="1" text="这是一个CheckBox." left="0"/>
    <radio id="rdo1" row="4" col="1" text="这是一个RadioBox1." value="1" left="0"/>
    <radio id="rdo2" row="5" col="1" text="这是一个RadioBox2." value="2" left="0"/>
    <radio id="rdo3" row="6" col="1" text="这是一个RadioBox3." value="3" left="0"/>
    <label id="lbl2" row="7" col="1" left="0"/>
    <line id="line1" row="7" col="2" left="0" right="0" top="0" bottom="0"/>
    <img id="img1" src="01.png" row="8" col="1" left="0" top="0"/>
    <chart id="cht1" row="8" col="1" left="0" right="0" top="0" bottom="0"/>
    <tickprovider id="tick_provider1" src="data.db"/>

  </layer>

  <dialog id="dialog1" width="400" height="300"/>
</window>